<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.runtime.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.xml"
            file-extensions="xml"
            id="edu.uci.isr.archstudio4.xadlContentBinding"
            name="xADL"
            priority="normal">
         <describer
               class="org.eclipse.core.runtime.content.XMLRootElementContentDescriber2">
            <parameter
                  name="element"
                  value="xArch">
            </parameter>
         </describer>
      </content-type>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="trese.taf.atf.RepositoryViewModel"
            objectClass="net.ample.tracing.ui.models.RepositoryViewModel">
         <action
               class="trese.taf.atf.PrologExportAction"
               enablesFor="1"
               id="trese.taf.atf.prologExport"
               label="Export prolog facts"
               menubarPath="contextAdditions"
               style="push">
         </action>
      </objectContribution>
   </extension>

</plugin>
